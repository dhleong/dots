{
    "coc.preferences.formatOnSaveFiletypes": ["python", "rust"],
    "coc.preferences.promptInput": false,
    "coc.source.OmniSharp.enable": true,
    "coc.source.OmniSharp.triggerCharacters": ".",
    "diagnostic.errorSign": "❌",
    "diagnostic.warningSign": "⚠️ ",
    "suggest.triggerAfterInsertEnter": true,
    "suggest.keepCompleteopt": true,
    "javascript.suggestionActions.enabled": false,
    "javascript.showUnused": false,
    "typescript.showUnused": false,
    "rust-analyzer.procMacro.enable": true
}
